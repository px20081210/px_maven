<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.px.dao.ClassificationDao">

    <select id="findAll" resultType="Classification">
        select * from yx_sxtj;
    </select>
    <insert id="insert" parameterType="com.px.po.Classification">
        insert into yx_sxtj (classCode,propertyName) values(#{classCode},#{propertyName});
    </insert>
</mapper>



